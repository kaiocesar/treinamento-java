package com.excript.aula0015;

import java.util.Scanner;

public class Aula0015 {

	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		
		final double velocidade_som = 340.29; // m/s
		double espaco;
		
		System.out.println("Entre com o espa√ßo tempo: ");
		espaco = in.nextDouble();
		
		System.out.println("Velocidade em metros " + (velocidade_som * espaco) );
		
		
		
	}
	
}
